{
	"name": "nickdekruijk/laravel-visitors",
	"keywords": [
		"laravel",
		"visitors",
		"tracking",
		"pageviews",
		"analytics"
	],
	"description": "A simple, privacy friendly visitor tracking for Laravel applications.",
	"license": "MIT",
	"authors": [
		{
			"name": "Nick de Kruijk",
			"email": "git@nickdekruijk.nl"
		}
	],
	"require": {
		"php": ">=8.2",
        "jenssegers/agent": "^2.6",
        "torann/geoip": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"NickDeKruijk\\LaravelVisitors\\": "src/"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"laravel": {
			"providers": [
				"NickDeKruijk\\LaravelVisitors\\ServiceProvider"
			],
			"aliases": {
				"Leap": "NickDeKruijk\\LaravelVisitors\\Facade"
			}
		}
	}
}